{
  "name": "fml",
  "displayName": "Flutter Markup Language",
  "description": "Flutter Markup Language VSCode Extension",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Snippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fml.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "xml",
        "aliases": [
          "XML",
          "xml",
          "FML",
          "fml",
          "Flutter Markup Language"
        ],
        "extensions": [
          ".xml",
          ".fml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "properties": {
      "languageServerExample.maxNumberOfProblems": {
        "scope": "resource",
        "type": "number",
        "default": 100,
        "description": "Controls the maximum number of problems produced by the server."
      },
      "languageServerExample.trace.server": {
        "scope": "window",
        "type": "string",
        "enum": [
          "off",
          "messages",
          "verbose"
        ],
        "default": "off",
        "description": "Traces the communication between VS Code and the language server."
      }
    },
    "grammars": [
      {
        "language": "xml",
        "scopeName": "source.fml",
        "path": "./syntaxes/fml.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "xml",
        "path": "./snippets/data-interface-widget-snippets/chart.json"
      },
